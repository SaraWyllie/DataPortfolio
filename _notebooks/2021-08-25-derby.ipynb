  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "#function to calculate win ratio.  takes a dataframe and team name\n",
    "def win_rat (df, team):\n",
    "    tdf=df[df['Team']==team]\n",
    "    tot_bouts=tdf.groupby(['year'])['win'].size()\n",
    "    tot_wins=tdf[tdf['win']=='win'].groupby(['year'])['win'].size()\n",
    "    win_ratio=tot_wins/tot_bouts\n",
    "    return win_ratio"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "#function to calculate points per bout\n",
    "def ppb (df, team):\n",
    "    tdf=df[df['Team']==team]\n",
    "    tot_bouts=tdf.groupby(['year'])['win'].size()\n",
    "    tot_points=tdf.groupby('year')['Team_score'].sum()\n",
    "    ppb=tot_points/tot_bouts\n",
    "    return ppb"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
  },
